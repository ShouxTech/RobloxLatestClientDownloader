<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>ClientDownloadLogo.ico</ApplicationIcon>
    <PackageIcon>ClientDownloadLogo.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="ClientDownloadLogo.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="ClientDownloadLogo.png" />
  </ItemGroup>

  <ItemGroup>
    <None Update="ClientDownloadLogo.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
